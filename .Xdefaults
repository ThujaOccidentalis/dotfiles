# Blur
URxvt.internalBorder:  13
#URxvt.borderColor: #8D7470
URxvt.borderColor: #798C35
URxvt.blurRadius:  15
URxvt.shading:  200
URxvt.tintColor:  white

URxvt.cursorBlink:  True

URxvt.externalBorder:  2

# Transparency
urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c800

urxvt*transparent: true
urxvt*shading: 30
